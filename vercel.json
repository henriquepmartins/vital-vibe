{
  "rewrites": [
    { "source": "/_expo/static/(.*)", "destination": "/_expo/static/$1" },
    { "source": "/assets/(.*)", "destination": "/assets/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "buildCommand": "expo export",
  "outputDirectory": "dist",
  "framework": "expo",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
