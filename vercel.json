{
  "rewrites": [{ "source": "/(.*)", "destination": "/" }],
  "buildCommand": "expo export",
  "outputDirectory": "dist",
  "framework": "expo",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
